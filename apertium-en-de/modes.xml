<?xml version="1.0" encoding="ISO-8859-15"?>
<modes>
  <mode name="en-de">
    <pipeline>
      <program name="lt-proc">
        <file name="en-de.automorf.bin"/>
      </program>
      <program name="apertium-tagger -g">
        <file name="en-de.prob"/>
      </program> 
      <program name="apertium-pretransfer"/>
      <program name="apertium-transfer">
        <file name="apertium-en-de.trules-en-de.xml"/>
        <file name="trules-en-de.bin"/>
        <file name="en-de.autobil.bin"/>
      </program>
      <program name="lt-proc -g">
        <file name="en-de.autogen.bin"/>
      </program>
<!--  <program name="lt-proc -p">
        <file name="en-de.autopgen.bin"/>
      </program> -->
    </pipeline>
  </mode>

  <mode name="de-en">
    <pipeline>
      <program name="lt-proc">
        <file name="de-en.automorf.bin"/>
      </program>
      <program name="apertium-tagger -g">
        <file name="de-en.prob"/>
      </program> 
      <program name="apertium-pretransfer"/>
      <program name="apertium-transfer">
        <file name="apertium-en-de.trules-de-en.xml"/>
        <file name="trules-de-en.bin"/>
        <file name="de-en.autobil.bin"/>
      </program>
      <program name="lt-proc -g">
        <file name="de-en.autogen.bin"/>
      </program>
      <program name="lt-proc -p">
        <file name="de-en.autopgen.bin"/>
      </program>
    </pipeline>
  </mode>

</modes>
