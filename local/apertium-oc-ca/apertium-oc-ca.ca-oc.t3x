		<rule comment="CHUNK: arregla en cv de relatiu que/qui">
			<pattern>
				<pattern-item n="qui"/>
			</pattern>
			<action>
				<out>
					<lu>
						<lit v="qui"/>
						<clip pos="1" part="a_rel"/>
						<clip pos="1" part="gen"/>
						<clip pos="1" part="nbr"/>
					</lu>
				</out>   
			</action>
		</rule> 

		<rule comment="CHUNK: canvia _sonque_ per _pas que_">
			<pattern>
				<pattern-item n="controla_adv"/>
			</pattern>
			<action>
				<choose>
					<when>
						<test>
							<equal>
								<clip pos="1" part="lem"/>
								<lit v="sonque"/>
							</equal>
						</test>
						<out>
							<lu>
								<lit v="pas"/>
								<lit-tag v="adv"/>
							</lu>
							<b/>
							<lu>
								<lit v="que"/>
								<lit-tag v="cnjsub"/>
							</lu>
						</out>  
					</when>
					<otherwise>
						<out>
							<lu>
								<clip pos="1" part="whole"/>
							</lu>
						</out>
					</otherwise>
				</choose>
			</action>
		</rule> 

			<rule comment="CHUNK: posa l'espai entre el pro i el verb">
