<?xml version="1.0" encoding="UTF-8"?>
<transfer>
<section-def-cats>
 <def-cat n="nom">
      <cat-item tags="n.*"/>
      <cat-item tags="np.*"/>
    </def-cat>
</section-def-cats>


<section-def-attrs>


    <def-attr n="pers">			<!-- Person -->
       <attr-item tags="p1"/>
       <attr-item tags="p2"/>
       <attr-item tags="p3"/>
    </def-attr>

 <def-attr n="a_det"> 			<!-- Determiners -->
      <attr-item tags="det.ind"/>
      <attr-item tags="det.def"/>
    </def-attr>

<def-attr n="nbr"> 			<!-- Number -->
   <attr-item tags="sg"/>
   <attr-item tags="pl"/>
   <attr-item tags="sp"/>
</def-attr>

 <def-attr n="gen">			<!-- Gender -->
      <attr-item tags="m"/>
      <attr-item tags="f"/>
      <attr-item tags="mf"/>
 </def-attr>
<def-attr n="a_nom">
   <attr-item tags="n"/>
   <attr-item tags="n.f"/>
   <attr-item tags="n.m"/>
   <attr-item tags="n.nt"/>
</def-attr>

</section-def-attrs>

<section-def-vars>
  <def-var n="number"/>
  <def-var n="genere"/>
</section-def-vars>

<section-rules>
<rule>
   <pattern>
     <pattern-item n="nom"/>
   </pattern>
   <action>
     <out>
       <lu>
         <clip pos="1" side="tl" part="lem"/>
         <clip pos="1" side="tl" part="a_nom"/>
 <!--	 <clip pos="1" side="tl" part="gen"/> -->
         <clip pos="1" side="tl" part="nbr"/>
       </lu>
     </out>
   </action>
</rule>

  </section-rules>

</transfer>
