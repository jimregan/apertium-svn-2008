      </program>
      <program name="apertium-postchunk">
        <file name="es-oc.t3x"/>
        <file name="es-oc.t3x.bin"/>
      </program>
      
      <program name="lt-proc $1">
        <file name="es-oc.autogen.bin"/>
      </program>
