                  <lit-tag v="sp"/>
                </equal>
              </not>
              <not>
                <equal>
                  <clip pos="1" side="tl" part="nbr"/>
                  <lit-tag v="PD"/>
                </equal>
              </not>
            </and>
          </test>
            <let>
                <clip pos="2" side="tl" part="nbr"/>
                <clip pos="1" side="tl" part="nbr"/>
              </let>
            </when>
						<otherwise>
            <let>
                <clip pos="2" side="tl" part="nbr"/>
                <lit-tag v="sg"/>
              </let>
						</otherwise>
          </choose>

    </def-macro>
