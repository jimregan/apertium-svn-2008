<?xml version="1.0" encoding="ISO-8859-15"?>
<modes>
  <mode name="gl-es">
    <pipeline>
      <program name="lt-proc">
        <file name="gl-es.automorf.bin"/>
      </program>
      <program name="apertium-tagger -g">
        <file name="gl-es.prob"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="apertium-transfer">
        <file name="apertium-es-gl.trules-gl-es.xml"/>
        <file name="trules-gl-es.bin"/>
        <file name="gl-es.autobil.bin"/>
      </program>
      <program name="lt-proc $1">
        <file name="gl-es.autogen.bin"/>
      </program>
      <program name="lt-proc -p">
        <file name="gl-es.autopgen.bin"/>
      </program>
    </pipeline>
  </mode>
  
  <mode name="es-gl">
    <pipeline>
      <program name="lt-proc">
        <file name="es-gl.automorf.bin"/>
      </program>
      <program name="apertium-tagger -g">
        <file name="es-gl.prob"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="apertium-transfer">
        <file name="apertium-es-gl.trules-es-gl.xml"/>
        <file name="trules-es-gl.bin"/>
        <file name="es-gl.autobil.bin"/>
      </program>
      <program name="lt-proc $1">
        <file name="es-gl.autogen.bin"/>
      </program>
      <program name="lt-proc -p">
        <file name="es-gl.autopgen.bin"/>
      </program>
    </pipeline>
  </mode>
</modes>
