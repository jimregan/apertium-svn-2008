<?xml version="1.0" encoding="iso-8859-15"?> <!-- -*- nxml -*- -->
<interchunk>
  <section-def-cats>

    <def-cat n="SN">
      <cat-item tags="SN.*"/>
    </def-cat>
    
    <def-cat n="rel">
      <cat-item tags="REL.*"/>
    </def-cat>
    
     
    <def-cat n="pr">
      <cat-item tags="PREP"/>
      <cat-item tags="PREP.*"/>
    </def-cat>
    
  

    <def-cat n="SV">
      <cat-item tags="SV.*"/>
    </def-cat>

    
    <def-cat n="vbcop">
      <cat-item tags="SV.vbcop.*"/>
      <cat-item tags="SV.vbser.*"/>
    </def-cat>
    
    <def-cat n="ADV">
      <cat-item lemma="adv" tags="adv"/>
      <cat-item lemma="adv" tags="adv.*"/>
      <cat-item lemma="no" tags="adv"/>
    </def-cat>

    <def-cat n="no">
      <cat-item lemma="no" tags="adv"/>
    </def-cat>

    <def-cat n="cnjsub">
      <cat-item lemma="cnjsub" tags="cnjsub"/>
    </def-cat>

    <def-cat n="quecnj">
      <cat-item lemma="que" tags="cnjsub"/>
      <cat-item lemma="que" tags="rel.an.mf.sp"/><!--para esta regla, si el tagger se equivoca, que entre igual. -->
    </def-cat>

  
    <def-cat n="SA">
      <cat-item tags="SA.*"/>
    </def-cat>


    <def-cat n="DET">
      <cat-item tags="DET.*"/>
    </def-cat>
    
    <def-cat n="NUM">
      <cat-item tags="NUM.*"/>
    </def-cat>

    <def-cat n="punt">
      <cat-item tags="sent"/>
    </def-cat>

  

  </section-def-cats>

  <section-def-attrs>
    

