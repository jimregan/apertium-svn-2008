                <lit-tag v="prn.subj.3.4.5"/>
                <lit v="$}"/>
              </chunk>
              <b/>

	  <b pos="3"/>
	  <chunk>
            <clip pos="3" part="whole"/>
	  </chunk>
	</out>
	</when>
	</choose>


      </action>
</rule>

