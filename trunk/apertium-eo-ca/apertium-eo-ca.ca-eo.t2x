<rule comment="REGLA: punt_cnjadv PRO SV SNacc (EO per l'acusatiu dels SN objecte amb changecase=nomi; em va donar el llibre :: (la knabino) al min doni la libron; el clitic passa a objecte indirecte precedit per al i el SN objecte agafa cas acusatiu)">
      <pattern>
	<pattern-item n="punt_cnjadv"/>
	<pattern-item n="SNobj"/>
	<pattern-item n="SVlex"/>
	<pattern-item n="SNacc"/>
      </pattern>
      <action>

	    <let>
	      <clip pos="4" part="case"/>
	      <lit-tag v="acc"/>
	    </let>
	    <let>
	      <clip pos="2" part="tipus_prn"/>
	      <lit-tag v="subj"/>
	    </let>
        <out>
          <chunk>
            <clip pos="1" part="whole"/>
          </chunk>
          <b pos="1"/>
        </out>


<!-- pro-drop-->

	<choose>
	  <when>
	    <test>
	      <not>
		<or>
		<equal>
		  <clip pos="3" part="temps"/>
		  <lit-tag v="imp"/>
		</equal>
		<equal>
		  <clip pos="3" part="temps"/>
		  <lit-tag v="inf"/>
		</equal>
		<equal>
		  <clip pos="3" part="temps"/>
		  <lit-tag v="ger"/>
		</equal>
		</or>
	      </not>
	    </test>
	    <out>
	      <chunk>
		<get-case-from pos="3">
		  <lit v="subj"/>
		</get-case-from>
		<lit-tag v="prn.subj"/>
		<clip pos="3" part="pers"/>
		<lit-tag v="GD"/>
		<clip pos="3" part="nbr"/>
		<lit v="{^"/>
		<get-case-from pos="3">
		  <lit v="prpers"/>
		</get-case-from>
		<lit-tag v="prn.subj.3.4.5"/>
		<lit v="$}"/>
	      </chunk>
	      <b/>	   
	    </out>
	    <choose>
	      <when>
		<test>
		  <equal>
		    <case-of pos="1" part="lem"/>
		    <lit v="Aa"/>
		  </equal>
		</test>
		<modify-case>
		  <clip pos="1" part="lem"/>
		  <lit v="aa"/>
		</modify-case>
	      </when>
	    </choose>
	  </when>
	</choose>
<!-- fi pro-drop-->

	<out>

	  <chunk>
	    <get-case-from pos="3">
	      <lit v="al"/>
	    </get-case-from>
	    <lit-tag v="pr"/>
	    <lit v="{^"/>
	    <get-case-from pos="3">
	      <lit v="al"/>
