	<pattern-item n="cnjadv"/>
	<pattern-item n="SNobj"/>
	<pattern-item n="vblike"/>
	<pattern-item n="ADV"/>	
	<pattern-item n="SNacc"/>	
      </pattern>
      <action>

	    <let> <!-- assignem nominatiu al SN inicial-->
	      <clip pos="2" part="case"/>
	      <lit-tag v="nomi"/>
	    </let>
	    <let>
	      <clip pos="2" part="tipus_prn"/>
	      <lit-tag v="subj"/>
	    </let>
	    <out>

 	  <chunk>
            <clip pos="1" part="whole"/>
          </chunk>
          <b pos="1"/>
	      <chunk><!--...creo un chunk que tindrà un pronom subjecte -->
		<lit v="prpers"/>
		<lit-tag v="SN.subj"/>
		<lit v="{^"/>
		<get-case-from pos="1">
		  <lit v="prpers"/>
		</get-case-from>
		<lit-tag v="prn.subj"/>
		<clip pos="2" part="pers"/>
		<lit-tag v="mf"/>
		<clip pos="2" part="nbr"/>						
		<lit v="$}"/>
	      </chunk>
	      <b pos="2"/>
	    </out>
	    <let>
	      <clip pos="5" part="case"/>
	      <lit-tag v="acc"/>
	    </let>
	    
	<out>
         <chunk>
            <clip pos="4" part="whole"/>
          </chunk>

          <b pos="3"/>

	  <chunk><!--envio el verb -->
	    <clip pos="3" part="lem"/>
	    <clip pos="3" part="tags"/>
	    <clip pos="3" part="chcontent"/>
	  </chunk>
	  <b pos="4"/>
	  <chunk><!-- el segon SN-->
	    <clip pos="5" part="lem"/>
	    <clip pos="5" part="tags"/>
	    <clip pos="5" part="chcontent"/>
	  </chunk>
	</out>
	<call-macro n="nombre_SN">
	  <with-param pos="4"/>
	</call-macro>	
      </action>
</rule>

