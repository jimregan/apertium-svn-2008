	  <chunk>
            <clip pos="2" part="lem"/>
            <clip pos="2" part="tags"/>
	    <clip pos="2" part="chcontent"/>
	  </chunk>
          <b pos="1"/> <!-- afegim el pronom subjecte -->

              <chunk>
                <get-case-from pos="3">
