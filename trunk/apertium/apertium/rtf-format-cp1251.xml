<?xml version="1.0" encoding="UTF-8"?>
<format name="rtf"> <!-- version 1.0 -->
  <options>
    <largeblocks size="8192"/>
    <input encoding="windows-1251"/>
    <output encoding="UTF-8"/>
    <tag-name regexp=""/>
    <escape-chars regexp='\\|[][&lt;&gt;@^$/]'/> 
<!--    <escape-chars regexp='[]]^@&lt;&gt;/]'/>  -->

    <space-chars regexp='[ \n\t\r$*]'/>
