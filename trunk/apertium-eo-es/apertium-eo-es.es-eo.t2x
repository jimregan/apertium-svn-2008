<!-- INDEX: cláusulas subordinadas volition (prodrop, tiempo subordinada)-->

<rule comment="REGLA: SVvolition cnjsub SV ( quiero que vengas - volas ke vi venu; la subordinada en imperativo. EOes)">
      <pattern>
	<pattern-item n="SVvolition"/>
	<pattern-item n="cnjsub"/>
	<pattern-item n="SV"/>
      </pattern>
      <action>
            <let>
              <clip pos="3" part="temps"/>
              <lit-tag v="imp"/>
            </let>

	<out>
	  <chunk>
	    <clip pos="1" part="whole"/>
	  </chunk>
	 <b/>
	  <chunk>
	    <clip pos="2" part="whole"/>
	  </chunk>
	  <b pos="1"/>
	  <chunk>
	    <get-case-from pos="3">
	      <lit v="subj"/>
	    </get-case-from>
	    <lit-tag v="prn.subj"/>
	    <clip pos="3" part="pers"/>
	    <lit-tag v="GD"/>
	    <clip pos="3" part="nbr"/>
	    <lit v="{^"/>
	    <get-case-from pos="3">
	      <lit v="prpers"/>
	    </get-case-from>
	    <lit-tag v="prn.subj.3.4.5"/>
	    <lit v="$}"/>
	  </chunk>
	  <b/>	   
	  <chunk>
            <clip pos="3" part="lem"/>
            <clip pos="3" part="tags"/>
            <clip pos="3" part="chcontent"/>
          </chunk>

	</out>
      </action>
</rule>


