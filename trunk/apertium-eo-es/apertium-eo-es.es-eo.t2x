<rule comment="REGLA: SV cnjsub SV ( . creo que vendrá -> . Mi kreas ke ĝi venos; EOes)">
      <pattern>
	<pattern-item n="punt_cnjadv"/>
	<pattern-item n="SV"/>
	<pattern-item n="cnjsub"/>
	<pattern-item n="SV_pers"/>
      </pattern>
      <action>

	<out>
	  <chunk>
	    <clip pos="1" part="whole"/>
	  </chunk>
	 <b/>
	</out>
        <choose>
          <when>
            <test>
              <not>
                <or>
                <equal>
                  <clip pos="2" part="temps"/>
                  <lit-tag v="imp"/>
                </equal>
                <equal>
                  <clip pos="2" part="temps"/>
                  <lit-tag v="inf"/>
                </equal>
                <equal>
                  <clip pos="2" part="temps"/>
                  <lit-tag v="ger"/>
                </equal>
                <equal>
                  <clip pos="2" part="temps"/>
                  <lit-tag v="pp"/>
                </equal>
                </or>
              </not>
            </test>
	   <out>
              <chunk>
                <get-case-from pos="2">
                  <lit v="subj"/>
                </get-case-from>
                <lit-tag v="prn.subj"/>
                <clip pos="2" part="pers"/>
                <lit-tag v="GD"/>
                <clip pos="2" part="nbr"/>
                <lit v="{^"/>
                <get-case-from pos="2">
                  <lit v="prpers"/>
                </get-case-from>
                <lit-tag v="prn.subj.3.4.5"/>
                <lit v="$}"/>
              </chunk>
              <b/>
          </out>
	  </when>
	  </choose>

          <out>
	  <chunk>
	    <get-case-from pos="3">
	    <clip pos="2" part="whole"/>
	    </get-case-from>
