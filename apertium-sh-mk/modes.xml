<?xml version="1.0" encoding="UTF-8"?>
<modes>
  <mode name="sh-mk">
    <pipeline>
      <program name="lt-proc">
        <file name="sh-mk.automorf.bin"/>
      </program>
      <program name="apertium-tagger -g">
        <file name="sh-mk.prob"/>
      </program> 
      <program name="apertium-pretransfer"/>
      <program name="apertium-transfer">
        <file name="apertium-sh-mk.sh-mk.t1x"/>
        <file name="sh-mk.t1x.bin"/>
        <file name="sh-mk.autobil.bin"/>
      </program>
      <program name="apertium-interchunk">
        <file name="apertium-sh-mk.sh-mk.t2x"/>
        <file name="sh-mk.t2x.bin"/>
      </program>
      <program name="apertium-postchunk">
        <file name="apertium-sh-mk.sh-mk.t3x"/>
        <file name="sh-mk.t3x.bin"/>
      </program>

      <program name="lt-proc -g">
        <file name="sh-mk.autogen.bin"/>
      </program>

<!--  <program name="lt-proc -p">
        <file name="sh-mk.autopgen.bin"/>
      </program> -->
    </pipeline>
  </mode>

